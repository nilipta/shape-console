#ifndef CIRCLE_H
#define CIRCLE_H

#include "Shapes.h"

class Circle:public Shape
{
	public:
		Circle(double radIn, double centerIn, std::string nameIn);
		void area();
		double perimeter();
		void setParams(std::string str);
	private:
		double center;
		double radius;
		std::string shapeName;
};

#endif
